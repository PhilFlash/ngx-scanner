    "ng-packagr": "^4.4.0",
    "npm-install-peers": "^1.2.1",
    "protractor": "^5.3.1",
    "rxjs": "^6.3.3",
    "ts-node": "^7.0.1",
    "tsickle": "^0.33.1",
    "tslint": "^5.9.1",
    "typescript": "~3.1.3",
    "zone.js": "^0.8.26"
  },
